#include <iostream>
#include<math.h>

using namespace std;

int main()
{
    int a, b, c;
    int product;
    for(a = 1; a <= 500; a++)
    {
        for(b = 1; b <= 500; b++)
        {
            c = 1000-a-b;
            if(pow(a,2) + pow(b,2) - pow(c,2) == 0 && a<b)
            {
                product = a*b*c;
            
            }
        }
    }
    cout<<product;
    return 0;
}
