#include<iostream>

using namespace std;

int main()
{
    int start = 1, difference = 2;
    int sum = 1;
    for(int i = 2; i <= 501; i++)
    {
        for(int j = 1; j < 5; j++)
        {
            start = start + difference;
            sum = sum + start;
        }
        difference = difference + 2;
    }
    cout<< sum;
}
